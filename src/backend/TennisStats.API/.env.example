# Tennis Statistics API - Environment Configuration
# Copy this file to .env and fill in your values

# Database Connection
TENNISSTATS_ConnectionStrings__DefaultConnection=Host=localhost;Port=5432;Database=tennis_stats;Username=postgres;Password=your_password_here

# BallDontLie API
TENNISSTATS_ExternalApi__BallDontLie__ApiKey=your_api_key_here

# Logging (optional)
TENNISSTATS_Logging__LogLevel__Default=Information

# CORS Origins (optional - comma separated)
TENNISSTATS_Cors__AllowedOrigins=http://localhost:5173,http://localhost:3000
